{
  "configure_optimizers":{
	  "optimizer": {
		  "type":"Adam",
		  "lr": 0.0004,
		  "betas": [0.5, 0.999]
	  },
	  "lr_scheduler": {
		"monitor": "val_loss",
		"scheduler": {
		  "type":"ReduceLROnPlateau",
		  "mode": "min",
		  "factor": 0.5,
		  "threshold": 1e-8,
		  "threshold_mode": "rel",
		  "patience": 0,
		  "verbose": true
		}
	  }
   },
  "early_stopping": {
    "monitor": "val_loss",
	"patience": 5,
	"verbose": true,
	"mode": "min",
	"strict":false
  },
  "out_len": 15,
  "crop_size": [200, 200],
  "in_features": 188,
  "cuda": true,
  "train_batch_size": 16,
  "test_batch_size": 16,
  "max_epochs": 100,
  "early_stopping_patience": 5,
  "data_location": "/mnt/foci_detection.hdf5",
  "data_loader": "foci",
  "normalization": "none"
}
